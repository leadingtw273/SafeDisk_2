<div class="container">
    <table class="css_table table_class table table-striped table-bordered table-responsive " width="100%" id="table_id">
        <thead>
            <tr>
                <th class="text-center">#</th>
                <th class="text-center">連線ID</th>
                <th class="text-center">使用者</th>
                <th class="text-center">金鑰</th>
                <th class="text-center">USB名稱</th>
                <th class="text-center">網路驗證</th>
                <th class="text-center">同步值</th>
            </tr>
        </thead>
    </table>
</div>

<!-- new USB Modal -->
<div class="modal fade" id="myNewModal" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">新增 USB 資料</h4>
            </div>
            <div class="modal-body">
                <form id="newUsb-from">
                    <div class="input-group form-group">
                        <span class="input-group-addon">USB 數量:</span>
                        <input type="number" class="form-control" id="count" name="count">
                    </div>
                    <hr>
                    <button type="submit" class="btn btn-default">Submit</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </form>
            </div>
        </div>

    </div>
</div>

<!-- edit USB Modal -->
<div class="modal fade" id="myEditModal" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">編輯 USB 調變參數</h4>
            </div>
            <div class="modal-body">
                <form id="showUsb-from">
                    <div class="form-group">
                        <label class="control-label col-sm-2" for="text">key : </label>
                        <div class="col-sm-10">
                            <p id="key" class="form-control-static"></p>
                        </div>
                    </div>
                    <div class="input-group form-group">
                        <span class="input-group-addon">A1 : </span>
                        <input id="a1" type="text" class="form-control" name="a1" placeholder="（小數點後３位）">
                    </div>
                    <div class="input-group form-group">
                        <span class="input-group-addon">A2 : </span>
                        <input id="a2" type="text" class="form-control" name="a2" placeholder="（小數點後３位）">
                    </div>
                    <div class="input-group form-group">
                        <span class="input-group-addon">A3 : </span>
                        <input id="a3" type="text" class="form-control" name="a3" placeholder="（小數點後３位）">
                    </div>
                    <hr>
                    <button type="submit" class="btn btn-default">更改</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </form>
            </div>
        </div>

    </div>
</div>


</div>


