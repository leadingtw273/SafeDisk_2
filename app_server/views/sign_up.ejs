<div class="row">
  <div class="panel-body formContainer">
    <form id="reg-form" class="form-horizontal">
      <div class="form-group">
        <label class="col-sm-4 control-label" for="email">Email</label>
        <div class="col-sm-5">
          <input class="form-control" type="text" placeholder="Email address" id="email" name="email">
        </div>
      </div>
      <div class="form-group">
        <label class="col-sm-4 control-label" for="phone">Phone</label>
        <div class="col-sm-5">
          <input class="form-control" type="text" placeholder="Phone" id="phone" name="phone">
        </div>
      </div>
      <div class="form-group">
        <label class="col-sm-4 control-label" for="username">Username</label>
        <div class="col-sm-5">
          <input class="form-control" type="text" placeholder="Username" id="username" name="username">
        </div>
      </div>
      <div class="form-group">
        <label class="col-sm-4 control-label" for="password">Password</label>
        <div class="col-sm-5">
          <input class="form-control" type="password" placeholder="Password" id="password" name="password">
        </div>
      </div>
      <div class="form-group">
        <label class="col-sm-4 control-label" for="verifypassword">Re-enter password</label>
        <div class="col-sm-5">
          <input class="form-control" type="password" placeholder="Re-enter password" id="verifypassword" name="verifypassword">
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-9 col-sm-offset-4">
          <button type="submit" class="btn btn-primary form-save" name="signup1" value="Sign up">Sign up</button>
        </div>
      </div>
    </form>
  </div>
</div>


<link rel="stylesheet" href="./stylesheets/signup.css">

<script src="./js/signup_valiation.js" rel="js" type="text/javascript"></script>